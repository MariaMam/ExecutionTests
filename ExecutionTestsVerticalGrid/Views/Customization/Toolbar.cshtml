@model IEnumerable

<script type="text/javascript">
    function OnToolbarItemClick(s, e){
        if(e.item.name == "All" || e.item.name == "Distinct")
            VerticalGrid.PerformCallback();
    }
    function OnBeginCallback(s, e){
        e.customArgs["isAllView"] = VerticalGrid.GetToolbar(0).GetItemByName("All").GetChecked();
    }
</script>

@Html.Partial("ToolbarPartial", Model)